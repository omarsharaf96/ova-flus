{
  "name": "ova-flus",
  "version": "1.0.0",
  "description": "Cross-Platform Finance Management Application",
  "private": true,
  "workspaces": [
    "backend",
    "web"
  ],
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../web && npm install",
    "start:backend": "cd backend && npm start",
    "start:web": "cd web && npm start",
    "start:mobile": "cd mobile && flutter run",
    "build:web": "cd web && npm run build",
    "test:all": "npm run test:backend && npm run test:web",
    "test:backend": "cd backend && npm test",
    "test:web": "cd web && npm test"
  },
  "keywords": [
    "finance",
    "budget",
    "portfolio",
    "stocks",
    "cross-platform"
  ],
  "author": "",
  "license": "MIT"
}
